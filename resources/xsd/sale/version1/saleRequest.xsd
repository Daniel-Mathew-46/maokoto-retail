                                        <xs:element name="CustomNm" type="CustomNmString"/>
                                        <xs:element name="CustomEmail" type="CustomEmailString"/>
                                        <xs:element name="CustomPhNum" type="CustomPhNumString"/>
                                        <xs:element name="CustomVrn" type="CustomVrnString"/>
                                        <xs:element name="SlDevId" type="SlDevIdString"/>
                                        <xs:element name="SlDevNum" type="SlDevNumString"/>
                                        <xs:element name="SlDesc" type="SlDescString"/>
                                        <xs:element name="SlGenDt" type="SlGenDtDateTime"/>
                                        <xs:element name="SlGenBy" type="SlGenByString"/>
                                        <xs:element name="SlApvdDt" type="SlApvdDtDateTime"/>
                                        <xs:element name="SlApvdBy" type="SlApvdByString"/>
                                        <xs:element name="Ccy" type="CcyString"/>
                                        <xs:element name="SlPmTyp" type="SlPmTypString"/>
                                        <xs:element name="DlyCt" type="DlyCtDecimal"/>
                                        <xs:element name="GlCt" type="GlCtDecimal"/>
                                        <xs:element name="ZNum" type="ZNumString"/>
                                        <xs:element name="RctNum" type="RctNumString"/>
                                        <xs:element name="RctVrfNum" type="RctVrfNumString"/>
                                        <xs:element name="EfdSNum" type="EfdSNumString"/>
                                        <xs:element name="SlRefNum" type="SlRefNumString"/>
                                        <xs:element name="SlBulkRefNum" type="SlBulkRefNumString"/>
                                        <xs:element maxOccurs="unbounded" name="SaleItm">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="SlItmCode" type="SlItmCodeString"/>
                                                    <xs:element name="SlItmNm" type="SlItmNmString"/>
                                                    <xs:element name="SlItmTxCode" type="SlItmTxCodeString"/>
                                                    <xs:element name="BillItmAmt" type="BillItmAmtDecimal"/>
                                                    <xs:element name="BillItmEqvAmt" type="BillItmEqvAmtDecimal"/>
                                                    <xs:element name="SlItmAmt" type="SlItmAmtDecimal"/>
                                                    <xs:element name="SlItmEqvAmt" type="SlItmEqvAmtDecimal"/>
                                                    <xs:element name="SlItmTxAmt" type="SlItmTxAmtDecimal"/>
                                                    <xs:element name="SlItmTxEqvAmt" type="SlItmTxEqvAmtDecimal"/>
                                                    <xs:element name="SlItmDiscAmt" type="SlItmDiscAmtDecimal"/>
                                                    <xs:element name="SlItmDiscEqvAmt" type="SlItmDiscEqvAmtDecimal"/>
                                                    <xs:element name="SlItmCstAmt" type="SlItmCstAmtDecimal"/>
                                                    <xs:element name="SlItmCstEqvAmt" type="SlItmCstEqvAmtDecimal"/>
                                                    <xs:element name="SlItmPftAmt" type="SlItmPftAmtDecimal"/>
                                                    <xs:element name="SlItmPftEqvAmt" type="SlItmPftEqvAmtDecimal"/>
                                                    <xs:element name="SlItmQty" type="SlItmQtyDecimal"/>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="eRtlSignature" type="eRtlSignatureString"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!--=================Start Header Xds Validation=================-->
    <xs:simpleType name="SpRefNumString">
        <xs:restriction base="xs:string">
            <xs:minLength value="9"/>
            <xs:maxLength value="9"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SlReqIdString">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SpSysIdString">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SlClBckUrlString">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PymtClBckUrlString">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ZRClBckUrlString">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DevIdString">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SlBulkFlagString">
        <xs:restriction base="xs:string">
            <xs:enumeration value="false"/>
            <xs:enumeration value="true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SlRespFlagString">
        <xs:restriction base="xs:string">
            <xs:enumeration value="false"/>
            <xs:enumeration value="true"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="VrsString">
        <xs:restriction base="xs:string">
            <xs:enumeration value="1.0"/>
        </xs:restriction>
    </xs:simpleType>

    <!--=================End Header Xsd Validation=================-->
    <!--=================Start Message Payload Xsd Validation======-->
    <xs:simpleType name="SlIdString">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SlTxIdNumString">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SubSpRefNumString">
        <xs:restriction base="xs:string">
            <xs:minLength value="6"/>
            <xs:maxLength value="6"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RtlCentCodeString">
        <xs:restriction base="xs:string">
            <xs:minLength value="6"/>
            <xs:maxLength value="6"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="BillAmtDecimal">
        <xs:restriction base="xs:decimal">
            <xs:minExclusive value="0"/>
            <xs:totalDigits value="38"/>
            <xs:fractionDigits value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BillEqvAmtDecimal">
        <xs:restriction base="xs:decimal">
            <xs:minExclusive value="0"/>
            <xs:totalDigits value="38"/>
            <xs:fractionDigits value="2"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SlAmtDecimal">
        <xs:restriction base="xs:decimal">
            <xs:minExclusive value="0"/>
            <xs:totalDigits value="38"/>
            <xs:fractionDigits value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SlTxAmtDecimal">
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="38"/>
            <xs:fractionDigits value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SlTxEqvAmtDecimal">
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="38"/>
            <xs:fractionDigits value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SlDiscAmtDecimal">
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="38"/>
            <xs:fractionDigits value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SlDiscEqvAmtDecimal">
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="38"/>
            <xs:fractionDigits value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SlRtAmtDecimal">
        <xs:restriction base="xs:decimal">
            <xs:minExclusive value="0"/>
            <xs:totalDigits value="38"/>
            <xs:fractionDigits value="2"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SlCstAmtDecimal">
        <xs:restriction base="xs:decimal">

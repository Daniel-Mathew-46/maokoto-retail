                                        <xs:element name="ZRClBckUrl" type="ZRClBckUrlString"/>
                                        <xs:element name="DevId" type="DevIdString"/>
                                        <xs:element name="SlBulkFlag" type="SlBulkFlagString"/>
                                        <xs:element name="SlRespFlag" type="SlRespFlagString"/>
                                        <xs:element name="Vrs" type="VrsString"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="SaleDtl">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="SlId" type="SlIdString"/>
                                        <xs:element name="SlTxIdNum" type="SlTxIdNumString"/>
                                        <xs:element name="SubSpRefNum" type="SubSpRefNumString"/>
                                        <xs:element name="RtlCentCode" type="RtlCentCodeString"/>
                                        <xs:element name="SpSysId" type="SpSysIdString"/>
                                        <xs:element name="BillAmt" type="BillAmtDecimal"/>
                                        <xs:element name="BillEqvAmt" type="BillEqvAmtDecimal"/>
                                        <xs:element name="SlAmt" type="SlAmtDecimal"/>
                                        <xs:element name="SlEqvAmt" type="SlEqvAmtDecimal"/>
                                        <xs:element name="SlTxAmt" type="SlTxAmtDecimal"/>
                                        <xs:element name="SlTxEqvAmt" type="SlTxEqvAmtDecimal"/>
                                        <xs:element name="SlDiscAmt" type="SlDiscAmtDecimal"/>
                                        <xs:element name="SlDiscEqvAmt" type="SlDiscEqvAmtDecimal"/>
                                        <xs:element name="SlCstAmt" type="SlCstAmtDecimal"/>
                                        <xs:element name="SlCstEqvAmt" type="SlCstEqvAmtDecimal"/>
                                        <xs:element name="SlPftAmt" type="SlPftAmtDecimal"/>
                                        <xs:element name="SlPftEqvAmt" type="SlPftEqvAmtDecimal"/>
                                        <xs:element name="SlRtAmt" type="SlRtAmtDecimal"/>
                                        <xs:element name="SlQty" type="SlQtyDecimal"/>
                                        <xs:element name="SlItmNum" type="SlItmNumDecimal"/>
                                        <xs:element name="SlDt" type="SlDtDateTime"/>
                                        <xs:element name="IdTyp" type="IdTypString"/>

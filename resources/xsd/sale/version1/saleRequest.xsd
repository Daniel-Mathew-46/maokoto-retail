    </xs:simpleType>

    <xs:simpleType name="SlGenByString">
        <xs:restriction base="xs:string">
            <xs:pattern value="([a-zA-Z0-9 _\\:.+=\-^,^;?|*\[\]{}()@/#$%^'~`&amp;&quot;&apos;&lt;&gt;\n\r]*)"/>
            <xs:minLength value="1"/>
            <xs:maxLength value="500"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SlApvdDtDateTime">
        <xs:restriction base="xs:dateTime">
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SlApvdByString">
        <xs:restriction base="xs:string">
            <xs:pattern value="([a-zA-Z0-9 _\\:.+=\-^,^;?|*\[\]{}()@/#$%^'~`&amp;&quot;&apos;&lt;&gt;\n\r]*)"/>
            <xs:minLength value="1"/>
            <xs:maxLength value="500"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="CcyString">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:pattern value="[A-Z]{3}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SlPmTypString">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:enumeration value="CASH"/>
            <xs:enumeration value="CHEQUE"/>
            <xs:enumeration value="CCARD"/>
            <xs:enumeration value="EMONEY"/>
            <xs:enumeration value="INVOICE"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DlyCtDecimal">
        <xs:restriction base="xs:decimal">
